function processJson(data){showLog(data,'JSON Params:');if(data.dfType=='promocodeRedemptionForm'){clientParams.promotionId=data.promotion_id;makeRedemptionForm(clientParams)}
if(data.dfType=='countdownCode'&&data.countdownType!==3){if(typeof clientParams.doRedirectOnComplete!=='undefined')data.doRedirectOnComplete=clientParams.doRedirectOnComplete;if(typeof clientParams.destroy_on_complete!=='undefined')data.destroy_on_complete=clientParams.destroy_on_complete;if(typeof clientParams.hide_countdown!=='undefined'&&clientParams.hide_countdown==1){data.doRedirectOnComplete=0;data.destroy_on_complete=1}
runCountdownLogic(data)}
if(data.dfType=='optinForm'){appConfig.trackingPixelUrl=data.trackingPixelUrl;if(document.getElementById("survey-wrap")){setTimeout(function(){replaceFields(data);runOptinLogic(data)},2000)}else{replaceFields(data);runOptinLogic(data)}}
if(data.dfType=='pixelUrl'){loadPixelUrl(data.pixelUrl)}
