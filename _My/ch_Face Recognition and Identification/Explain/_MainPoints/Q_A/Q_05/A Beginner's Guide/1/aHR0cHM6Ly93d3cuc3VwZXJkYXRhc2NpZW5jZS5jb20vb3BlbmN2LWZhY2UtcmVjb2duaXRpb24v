function runCountdownLogic(countdownConfig){showLog(countdownConfig,'countdown config');if(countdownConfig.redirectToOptinNow==1&&countdownConfig.optinUrl!==null){window.onbeforeunload=null;window.location=addGetParamsToRedirectUrl(countdownConfig.optinUrl);return}
