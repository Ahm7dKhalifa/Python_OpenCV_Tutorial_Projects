function getLegacyParams(data){var dWrap=document.querySelectorAll('[data-scriptid]');var stepOne;var stepTwo;var keys;var extraConfig;var splittedURL;if(dWrap.length>0){var dfScript=dWrap}else{var dfScript=document.getElementsByTagName('script')}
